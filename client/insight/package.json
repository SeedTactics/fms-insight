{
  "name": "insight",
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "private": true,
  "sideEffects": [
    "./src/index.ts",
    "./src/demo.ts"
  ],
  "scripts": {
    "start": "parcel src/index.html",
    "build": "tsc --noEmit --composite false && parcel build src/index.html",
    "build-demo": "tsc --noEmit --composite false && parcel build --out-dir demo --public-url /demo/ src/demo.html",
    "test": "jest --ci --runInBand",
    "test:watch": "jest --watch",
    "nswag": "./node_modules/nswag/bin/nswag.js swagger2tsclient /runtime:NetCore20 /input:../../server/fms-insight-api.json /output:src/data/api.ts"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "copy-to-clipboard": "^3.0.0",
    "date-fns": "^2.0.0",
    "jdenticon": "^2.0.0",
    "moment": "^2.20.0",
    "oidc-client": "^1.5.3-beta.1",
    "prelude-ts": "^0.8.2",
    "query-string": "^6.0.0",
    "react": "^16.8.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-dates": "^21.0.0",
    "react-document-title": "^2.0.0",
    "react-dom": "^16.8.0",
    "react-qr-reader": "^2.1.0",
    "react-redux": "^7.0.0",
    "react-sortable-hoc": "^1.4.0",
    "react-timeago": "^4.0.0",
    "react-vis": "^1.8.2",
    "reconnecting-websocket": "^4.4.0",
    "redux": "^4.0.0",
    "redux-first-router": "^2.1.0",
    "reselect": "^4.0.0",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.0",
    "@testing-library/react": "^10.0.0",
    "@types/enzyme": "^3.1.8",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/faker": "^4.1.2",
    "@types/history": "^4.6.2",
    "@types/jest": "^25.0.0",
    "@types/node": "^13.0.0",
    "@types/react": "^16.8.2",
    "@types/react-beautiful-dnd": "^12.0.0",
    "@types/react-dates": "^17.1.5",
    "@types/react-document-title": "^2.0.0",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.0",
    "@types/redux-first-router": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-react": "^7.17.0",
    "faker": "^4.1.0",
    "hoist-non-react-statics": "^3.3.0",
    "jest": "^25.0.0",
    "nswag": "^12.3.1",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-sw-precache": "^1.0.1",
    "prettier": "^2.0.0",
    "source-map-explorer": "^2.1.0",
    "ts-jest": "^25.0.0",
    "typescript": "^3.8.0"
  },
  "jest": {
    "testURL": "http://localhost",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/.build/",
      "<rootDir>/.cache/"
    ],
    "moduleNameMapper": {
      ".*\\.css$": "<rootDir>/file-mock.js",
      ".*webrtc-adapter.*": "<rootDir>/file-mock.js"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      ".*\\.svg$": "<rootDir>/file-mock.js",
      ".*events-json.txt$": "<rootDir>/file-mock.js"
    },
    "preset": "ts-jest",
    "testMatch": null,
    "snapshotSerializers": [
      "<rootDir>/prelude-ts-snapshots.js"
    ]
  },
  "sw-precache": {
    "navigateFallback": "index.html",
    "navigateFallbackWhitelist": [
      "^(?!/(swagger|api|instructions)/).*"
    ]
  }
}
